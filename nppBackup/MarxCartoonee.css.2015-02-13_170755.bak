body{
background:url(http://i.imgur.com/3od51IS.png) #fce3a0 98% 95%  no-repeat fixed;
font-weight: light;
padding-top:100px;
}
table + br{
margin-top:20px;
}
@font-face{
font-family:Allura Regular;
src:url(https://dl.dropboxusercontent.com/u/50306385/MAL/Allura-Regular.ttf);
}
@font-face{
font-family:Cinnamon Cake;
src:url(https://dl.dropboxusercontent.com/u/50306385/MAL/cinnamon%20cake.ttf);
}

/* LIST FONTS
This is the type and color for more of the numbers, links, and words on the list itself! */

.td1, .td2, a, a:visited, .category_totals, .table_header, #grand_totals, #copyright {
color:white;
font-family:Cinnamon Cake;
font-size:12pt;
}
/* OTHER CODES
Important codes for the layout's setup. Don't mess with these unless you know exactly what you're doing. If you want to customize more on the page, use the link at the top of this CSS, or ask in my club! */

#list_surround {
/*width:950px;*/
margin:0 33% auto 3%;
opacity:0.75;
position:relative;
z-index:0;
}
/*#mal_control_strip:before{
content:" ";
margin:0;
position:fixed;
width:100%;
height:92px;
z-index:1;
background:url(http://i.imgur.com/X5mwGIL.png) 0 0 repeat-x;
}*/
#mal_control_strip{
background:transparent url(http://i.imgur.com/X5mwGIL.png) 0 0 repeat-x !important;
height:92px;
padding-bottom:52px !important;
position:fixed;
z-index:1;
top:0;
}
#mal_cs_pic{
background-color:#2E51A2;
}
#mal_cs_otherlinks div strong{
color:#694c00 !important;
}
#mal_cs_listinfo a,#mal_cs_links a,#mal_cs_otherlinks a,#mal_cs_powered a{
color:black !important;
}
#mal_cs_listinfo a:hover,#mal_cs_links a:hover,#mal_cs_otherlinks a:hover,#mal_cs_powered a:hover{
color:black !important;
}
a ,a:visited {
text-decoration:none;
}
a:hover, a:visited:hover {
color:#cc6a76;
}
.category_totals,.td1,.td2,#grand_totals,#copyright {
padding:2px;
}
/* MINE */
.animetitle, .animetitle:visited {
color:#ed7b35;
font-family:Monotype Corsiva;
font-size:14pt;
}
.animetitle:hover,.animetitle:visited:hover{
color:#f7b664;
text-decoration:none;
/*text-shadow:-1px -1px 0 #ed7b35/*,1px -1px 0 #ed7b35,-1px 1px 0 #ed7b35,1px 1px 0 #ed7b35*/;
}
.List_LightBox,.List_LightBox + a{
color:#ed7b35;
}
.td1,.td2,.table_header:first-child,.table_header:last-child,.header_title,.category_totals{
border-width:0px;
border-style:solid;
border-color:#694c00;
}
.table_header {
background-color:#7d6042;
/*font-family:Cinnamon Cake;
font-size:8pt;*/
border-width:0;
font-weight:bold;
padding:2px;
}
.header_title {
background:url(http://i.imgur.com/A36whwP.png) #DEAC67 200px center no-repeat;
color:white;
font-family:Allura Regular/*Vladimir Script*/;
font-size:24pt;
font-weight:bold;
padding:5px;
/*text-shadow:-2px -2px 0 #DEAC67,2px -2px 0 #DEAC67,-2px 2px 0 #DEAC67,2px 2px 0 #DEAC67;*/
}
.header_title span{
display:block;
width:190px;
text-align:center;
}
.header_title span:after{
content:" ";
background-image:url(http://i.imgur.com/jrZiofS.png);
width:230px;
height:156px;
vertical-align:baseline;
position:absolute;
left:100%;
transform:translateY(-40%) translateX(-50px);
z-index:1;
pointer-events:none;
}
.td1,.td2{
color:#636468;
background-color:#fff6cf;
border-right-width:0px;
}
.td2{
background-color:#fcecb9;
}
.td1:nth-child(3),.td1:nth-child(5),.td2:nth-child(3),.td2:nth-child(5){
color:white;
background-color:#edc083;
font-weight:bold;
}
.td2:nth-child(3),.td2:nth-child(5){
background-color:#DEAC67;
}
.td2:first-child,.td1:first-child,.table_header:first-child{
border-left-width:5px !important;
}
.td2:last-child,.td1:last-child,.table_header:last-child{
border-right-width:5px;
}
.header_title{
border-width:5px 5px 0px 5px;
border-radius:10px 10px 0 0;
}
.category_totals{
color:white;
background-color:#7d6042;
border-width:0px 5px 5px 5px;
border-radius:0 0 10px 10px;
padding-left:8px;
/*font-family:Cinnamon Cake;
font-size:10pt;*/
}
.status_selected ,.status_not_selected {
background-color:#694c00;
padding:2px;
color:#b8b9bd;
}
.status_selected a{
color:white;
font-family:Monotype Corsiva;
font-size:14pt;
}
.status_not_selected a{
color:#b8b9bd;
font-family:Monotype Corsiva;
font-size:14pt;
}
.status_selected:first-child,.status_not_selected:first-child{
border-width:0;
border-radius:5px 0 0 5px;
}
.status_selected ,.status_not_selected {
border-right:1px solid #fff6cf;
border-image:linear-gradient(to bottom, #694c00 0%,#fff6cf 50%,#694c00 100%) 1 1 !important;
}
.status_selected:last-child,.status_not_selected:last-child{
border:none;
border-radius:0 5px 5px 0;
}

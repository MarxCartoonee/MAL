.status_selected ,.status_not_selected {
background-color:#694c00;
padding:2px;
color:#b8b9bd;
}
.status_selected a{
color:white;
font-family:Monotype Corsiva;
font-size:14pt;
}
.status_not_selected a{
color:#b8b9bd;
font-family:Monotype Corsiva;
font-size:14pt;
}
.status_selected:first-child,.status_not_selected:first-child{
border-width:0;
border-radius:5px 0 0 5px;
}
.status_selected ,.status_not_selected {
border-right:1px solid #fff6cf;
border-image:linear-gradient(to bottom, #694c00 0%,#fff6cf 50%,#694c00 100%) 1 1 !important;
}
.status_selected:last-child,.status_not_selected:last-child{
border-width:0;
border-radius:0 5px 5px 0;
}
.thickbox {
color:cyan;
font-family:fantasy;
font-size:12px;
}
.table_header {
border-width:0;
font-weight:bold;
padding:2px;
}
.category_totals {
height:30px;
}
#copyright, #grand_totals {
text-align: center;
margin:0 auto;
}
